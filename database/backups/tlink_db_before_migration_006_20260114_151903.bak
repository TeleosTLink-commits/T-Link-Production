--
-- Name: samples_with_status; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.samples_with_status AS
 SELECT id,
    chemical_name,
    received_date,
    lot_number,
    quantity,
    concentration,
    has_dow_sds,
    cas_number,
    has_coa,
    certification_date,
    recertification_date,
    expiration_date,
    un_number,
    hazard_description,
    hs_code,
    hazard_class,
    packing_group,
    packing_instruction,
    status,
    notes,
    created_by,
    created_at,
    updated_at,
    search_vector,
        CASE
            WHEN ((status)::text = 'depleted'::text) THEN 'Depleted'::text
            WHEN ((status)::text = 'archived'::text) THEN 'Archived'::text
            WHEN (expiration_date IS NULL) THEN 'Active'::text
            WHEN (expiration_date < CURRENT_DATE) THEN 'Expired'::text
            WHEN (expiration_date < (CURRENT_DATE + '30 days'::interval)) THEN 'Expiring Soon (30 days)'::text
            WHEN (expiration_date < (CURRENT_DATE + '60 days'::interval)) THEN 'Expiring Soon (60 days)'::text
            WHEN (expiration_date < (CURRENT_DATE + '90 days'::interval)) THEN 'Expiring Soon (90 days)'::text
            ELSE 'Active'::text
        END AS expiration_status,
        CASE
            WHEN ((expiration_date IS NOT NULL) AND (expiration_date < CURRENT_DATE)) THEN (expiration_date - CURRENT_DATE)
            ELSE NULL::integer
        END AS days_until_expiration
   FROM public.samples s;


