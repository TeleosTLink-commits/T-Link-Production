-- Name: test_methods; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.test_methods (
    id uuid DEFAULT gen_random_uuid() NOT NULL,
    legacy_number character varying(100),
    legacy_lab_source character varying(100),
    original_title character varying(255),
    official_number character varying(50),
    official_title character varying(255),
    category_id uuid,
    method_type character varying(100),
    status character varying(50) DEFAULT 'draft'::character varying,
    verification_status character varying(50) DEFAULT 'pending'::character varying,
    verified_by uuid,
    verified_date date,
    verification_notes text,
    purpose text,
    scope text,
    principle text,
    equipment_required text,
    reagents_required text,
    procedure text,
    calculations text,
    acceptance_criteria text,
    method_references text,
    current_version character varying(20) DEFAULT '1.0'::character varying,
    version_notes text,
    supersedes_method_id uuid,
    superseded_by_method_id uuid,
    effective_date date,
    review_date date,
    next_review_date date,
    created_by uuid,
    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    updated_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    sample_matrix character varying(200),
    analyte character varying(200),
    test_purpose character varying(100),
    file_path character varying(500),
    file_name character varying(255)
);


ALTER TABLE public.test_methods OWNER TO postgres;

--
-- Name: test_methods_full; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.test_methods_full AS
 SELECT id,
    legacy_number,
    legacy_lab_source,
    original_title,
    official_number,
    official_title,
    COALESCE(official_title, original_title) AS display_title,
    COALESCE(official_number, legacy_number) AS display_number,
    status,
    verification_status,
    current_version,
    method_type,
    effective_date,
    verified_date AS aal_verification_date,
    created_at,
    updated_at,
    file_path,
    file_name,
        CASE
            WHEN (official_number IS NOT NULL) THEN true
            ELSE false
        END AS is_standardized
   FROM public.test_methods tm;


ALTER VIEW public.test_methods_full OWNER TO postgres;

--
-- Name: users; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.users (
    id uuid DEFAULT public.uuid_generate_v4() NOT NULL,
    username character varying(100) NOT NULL,
    email character varying(255) NOT NULL,
    password_hash character varying(255) NOT NULL,
    first_name character varying(100) NOT NULL,
    last_name character varying(100) NOT NULL,
    role character varying(50) NOT NULL,
    is_active boolean DEFAULT true,
    last_login timestamp without time zone,
    created_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    updated_at timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    email_verified boolean DEFAULT false,
    failed_login_attempts integer DEFAULT 0,
    locked_until timestamp without time zone,
    CONSTRAINT users_role_check CHECK (((role)::text = ANY ((ARRAY['admin'::character varying, 'lab_staff'::character varying, 'logistics'::character varying, 'manufacturer'::character varying])::text[])))
);


ALTER TABLE public.users OWNER TO postgres;

