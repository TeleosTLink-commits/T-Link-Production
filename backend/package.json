        "@types/morgan": "^1.9.10",
        "@types/multer": "^1.4.13",
        "@types/node": "^20.19.27",
        "@types/node-cron": "^3.0.11",
        "@types/nodemailer": "^6.4.21",
        "@types/pdfkit": "^0.13.4",
        "@types/pg": "^8.16.0",
