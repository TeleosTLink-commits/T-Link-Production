{
    "name": "t-link-backend",
    "version": "1.0.0",
    "description": "T-Link Backend API",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "test": "jest --detectOpenHandles --forceExit",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:unit": "jest --testPathPattern='__tests__/unit'",
        "test:integration": "jest --testPathPattern='__tests__/integration'",
        "db:migrate": "ts-node src/database/migrate.ts",
        "db:seed": "ts-node src/database/seed.ts",
        "db:test:setup": "ts-node src/database/testSetup.ts",
        "migrate:file-columns": "ts-node src/scripts/addFileColumnsMigration.ts",
        "migrate:sample-files": "ts-node src/scripts/runSampleFileMigration.ts",
        "fix:admin-user": "ts-node src/scripts/fixAdminUser.ts",
        "import:test-methods": "ts-node src/scripts/importTestMethods.ts",
        "migrate:cloudinary": "ts-node src/scripts/migrateCloudinary.ts",
        "upload:cloudinary:production": "ts-node src/scripts/uploadToCloudinaryProduction.ts",
        "upload:samples": "ts-node src/scripts/uploadSampleFiles.ts",
        "verify:production": "ts-node src/scripts/verifyProduction.ts"
    },
    "dependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.25",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/ms": "^2.1.0",
        "@types/multer": "^1.4.13",
        "@types/node": "^20.19.27",
        "@types/node-cron": "^3.0.11",
        "@types/nodemailer": "^6.4.21",
        "@types/pdf-parse": "^1.1.5",
        "@types/pdfkit": "^0.13.4",
        "@types/pg": "^8.16.0",
        "@types/uuid": "^10.0.0",
        "axios": "^1.7.9",
        "bcrypt": "^6.0.0",
        "cloudinary": "^2.8.0",
        "cors": "^2.8.5",
        "csv-parse": "^6.1.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.3.1",
        "helmet": "^7.1.0",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.3",
        "nodemailer": "^7.0.11",
        "pdf-parse": "^2.4.5",
        "pdfkit": "^0.13.0",
        "pg": "^8.16.3",
        "tesseract.js": "^7.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/supertest": "^6.0.2",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1"
    }
}
